{
  "presets": [
  	[
      "env",
    	{
        "targets": {
          "uglify": true
        },
        "useBuiltIns": true,
        "modules": false,
        "debug": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-function-bind",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": [
      ]
    },
    "testing": {
      "presets": ["react-optimize"]
    },
    "staging": {
      "presets": ["react-optimize"]
    },
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
